{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },

    "globals": {
        "React": true
    },

    "ecmaFeatures": {
        "jsx": true
    },

    "plugins": [
        "react"
    ],

    "extends": "airbnb",

    "rules": {
        "semi": ["error", "never"]
    }
}
